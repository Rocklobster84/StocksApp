<div class="ui pagination menu">
    <% if (pages && pages > 0) { %>
        <% if (current == 1) { %>
            <a class="disable item">First</a>
        <% } else { %>
            <a class="item" href="/stocks<%if(search){%>?search=<%=search%><%}%>">First</a>
        <% } %>

        <% if (current == 1) { %>
            <a class="disabled item"><<</a>
        <% } else { %>
            <a class="item" href="/stocks?page=<%= Number(current) - 1 %><%if(search){%>&search=<%=search%><%}%>"><<</a>
        <% } %>

        <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
        <% if (i !== 1) { %>
            <a class="disabled item">...</a>
        <% } %>
        <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
        <% if (i == current) { %>
            <a class="active item"><%= i %></a>
        <% } else { %>
            <a class="item" href="/stocks?page=<%= i %><%if(search){%>&search=<%=search%><%}%>"><%= i %></a>
        <% } %>
        <% if (i == Number(current) + 4 && i < pages) { %>
            <a class="disabled item">...</a>
        <% } %>
        <% } %>

        <% if (current == pages) { %>
            <a class="disabled item">>></a>
        <% } else { %>
            <a class="item" href="/stocks?page=<%= Number(current) + 1 %><%if(search){%>&search=<%=search%><%}%>">>></a>
        <% } %>

        <% if (current == pages) { %>
            <a class="disabled item">Last</a>
        <% } else { %>
            <a class="item" href="/stocks?page=<%= pages %><%if(search){%>&search=<%=search%><%}%>">Last</a>
        <% } %>
    <% } %>
</div>